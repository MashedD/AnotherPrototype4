; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Another Prototype 4"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.1")
config/icon="res://icon.png"

[autoload]

SceneManager="*res://addons/scene_manager/scene_manager.tscn"
Scenes="*res://addons/scene_manager/scenes.gd"
SceneManagerGlobals="*res://sources/autoload/scene_manager_globals.gd"
FileUtils="*res://sources/autoload/file_utils.gd"
ArrayUtils="*res://sources/autoload/array_utils.gd"
MaterialUtils="*res://sources/autoload/material_utils.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Another Prototype 4"

[editor_plugins]

enabled=PackedStringArray("res://addons/SmoothScroll/plugin.cfg", "res://addons/scene_manager/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
vulkan/rendering/back_end=1
